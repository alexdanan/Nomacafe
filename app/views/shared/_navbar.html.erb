  <!--[+] PROFILE AVATAR-->
  <div class="profile-hedader">
    <div class="profile-thumb">
      <% if current_user.photo.attached? %>
        <div class="thumb" style="background-image:url('<%= cl_image_path current_user.photo.key %>')"></div>
      <% else %>
      <div class="thumb" style="background-image:url('https://kitt.lewagon.com/placeholder/users/arthur-littm')"></div>
    <% end %>
      <div class="profile-name">@<%= current_user.user_name %></div>
    </div>
  </div>



  <!--[-]<%  %>-->
    
  <div data-controller="togglebell">
    <div class="header-bell">
      <div data-action="click->togglebell#toggle">
        <% if current_user.notifications.empty? %>
          <i class="fas fa-bell bell-icon" ></i>
        <% else %>
          <i class="fas fa-bell bell-icon-notif">
            <i class="fas fa-circle"></i>
          </i>
        <% end %>

        <div class="d-none position-absolute notifications-drop-down" data-togglebell-target="dropdownnotifs" >

          <% current_user.notifications.each do |notification| %>
            <div class="notification">
              <img src='<%= cl_image_path Booking.find(notification.notifiable_id).user.photo.key %>' class="avatar-large" />
            <% if notification.key == "booking.owner" %>
              <div class="notification-content">
                <p><small>@<%= Booking.find(notification.notifiable_id).user.user_name %> has just booked <span style="color: $nomacafe;"><%= Booking.find(notification.notifiable_id).table.name %></span> </small></p>
              </div>
            <% else %>
              <div class="notification-content">
                <p><small>@<%= Booking.find(notification.notifiable_id).user.user_name %> has joined your table!</small></p>
              </div>
            <% end %>
          </div>
        <% end %>
      </div>

      </div>
    </div>
  </div>
